body,h1,h2,h3,p,ul,ol,form,fieldset,figure {
    margin: 0;
    padding: 0;
}
div,figure,img {
    box-sizing: border-box;
}
body {
    background-color: #f8f8f9;
    font-family: "Helvetica Neue", Helvetica, Arial, "Microsoft Yahei UI", "Microsoft YaHei", SimHei, "\5B8B\4F53", simsun, sans-serif;

}
/*解决兼容手机浏览器(span a 等)点击蓝色*/
* {
    -webkit-tap-highlight-color: transparent;
    -webkit-appearance: none;
}
/*去掉 chrome input 的背景黄色*/
input:-webkit-autofill {
    -webkit-box-shadow: 0 0 0 1000px rgba(255, 255, 255, 0.5) inset !important;
}
input:-webkit-autofill {
    -webkit-animation-name: autofill;
    -webkit-animation-fill-mode: both;
}
@-webkit-keyframes autofill {
    to {
        color: #9b9b9b;
        background: transparent;
    }
}
/*input标签中placeholder显示*/
::-webkit-input-placeholder { /* WebKit browsers */
    color:    #9b9b9b;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color:    #9b9b9b;
    opacity:  1;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
    color:    #9b9b9b;
    opacity:  1;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
    color:    #9b9b9b;
}

input:focus{
    color: #4A4A4A;
}
input,
button,
select,
textarea {
    outline: none;
    border: 0;
}
ul,ol {
    list-style: outside none none;
}
li{list-style:none;}
input{-webkit-appearance:none; /*去除input默认样式*/ padding: 0;}
input[type="submit"],
input[type="reset"],
input[type="button"],
input{-webkit-appearance:none;}
input:focus{
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    -webkit-user-modify:read-write-plaintext-only;
}
/*去除IE10+浏览器文本框后面的小叉叉*/
input::-ms-clear {
    display: none;
}
input {outline:none; border: 0;color: #4A4A4A}
input::-webkit-input-placeholder {
    /* WebKit browsers */
    color: #9b9b9b;
}
input::-moz-placeholder {
    /* Mozilla Firefox 4 to 18 */
    color: #9b9b9b;
}
input::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color: #9b9b9b;
}
input:-ms-input-placeholder {
    /* Internet Explorer 10+ */
    color: #9b9b9b;
}
a{text-decoration: none}
a:hover{text-decoration: none}
textarea {
    resize: none
}

select::-ms-expand {
    display: none;
}

select {
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
}
input::-webkit-input-placeholder {
    /* WebKit browsers*/
    color:#9b9b9b;
}
input:-moz-placeholder {
    /* Mozilla Firefox 4 to 18*/
