body,a{
    text-decoration : none
}
ul,ol {
    list-style: outside none none;
}
a{text-decoration: none}
a:hover{text-decoration: none}
a:link,a:active ,a:visited,a:hover {text-decoration:none;}
li{list-style:none;}
.unified{
    width: 100%;float: left;
}
.margin0{
    margin:  0;
}
.float{
    float: left;
}
.display_inlibl{
    display: inline-block;
}
.fo-electricity{
    background: url("/res/images/imgv6/ydgl_n_icon.png")no-repeat;height: 20px;background-size: 100%;margin: 0 27px 0 0;
}
.active_nav_img{
   display: inline-block;width: 30px;height: 30px; margin-right: 27px;padding: 5px;
}

.dropdown-menu-settings{
    width: 100%;height: 30px;font-size: 14px;color: #000000;line-height: 30px;text-align: center;
}
.dropdown-bottom{
    -webkit-transform: rotate(-90deg);
    -ms-transform: rotate(-90deg);
    -o-transform: rotate(-90deg);
    transform: rotate(-90deg);margin-top: 3px;
}


.conter-warning-header{
    width: 100%;height: 40px;background: #FAFAFA;text-align: left;position: relative;
}
.conter-warning-top-img{
    width: 30px;height:30px;position: absolute;top: 0;left: 0;
}
.conter-warning-top-img2{
    width: 4px;height:22px;position: absolute;top: 10px;left: 0;
}
.conter-warning-header span{
    display:inline-block;font-size: 16px;line-height: 40px;color: #4a4a4a;float: left;margin-left: 25px;
}

.conter-warning-footer-tad{
    width: 100%;height: 44px;padding-left: 24px;font-size: 12px;color: #4a4a4a;line-height:44px;
    /*margin-top: 10px;*/
}
.conter-warning-footer-tad > span{
    display: inline-block;float: left;
}
.conter-warning-footer-tad > .span1{
    margin-right: 20px;
}
.conter-warning-footer-tad > .span2{
    margin-right: 30px;
}
.conter-warning-footer-tad > .span3{
    line-height: 14px;height: 40px;margin: 5px 0 0 0;
}
.content-all-wrapper-menu-top{
   /*background: white;margin: 0;*/
}
