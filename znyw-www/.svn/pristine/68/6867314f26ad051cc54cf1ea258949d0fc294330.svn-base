}
.content-all-wrapper-menu-top ul li a{
    display:block;width: 100%;height: 100%;text-align: center;font-size: 14px;color: #4a4a4a;line-height: 40px;
}
.content-all-wrapper-menu-top ul  .acs a{
    background: #1FBCeC;color: white;
}
.content-all-wrapper-menu-top ul li a:hover{
    background: #1FBCeC;color: white;
}

