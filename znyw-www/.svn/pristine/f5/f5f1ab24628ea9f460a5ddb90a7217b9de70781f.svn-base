html,
body {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
}
ul{list-style-type:none;margin: 0;padding: 0;}
li{list-style-type : none;}
body {
    Font-Family: "Hiragino Sans GB", "Microsoft Yahei", SimSun, Arial, "Helvetica Neue", Helvetica;
}
input {outline:none; border: 0;color: #4A4A4A}
input::-webkit-input-placeholder {
    /* WebKit browsers */
    color: #9b9b9b;
}
  input::-moz-placeholder {
      /* Mozilla Firefox 4 to 18 */
      color: #9b9b9b;
  }

input::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color: #9b9b9b;
}
  input:-ms-input-placeholder {
      /* Internet Explorer 10+ */
      color: #9b9b9b;
  }
a{text-decoration: none;}
.body_img{
    background: url(../images/img2/snow.jpg)no-repeat center center;
    min-width: 700px;
}
.reg_center{
    width: 100%;
    padding-top: 117px;
}
.reg_nav{
    width: 100%;
}
.reg_nav_img{
    width: 68px;
    height: 76px;
    margin: auto;
    background: url(../images/img2/logo_white68X76.png)no-repeat center center;
}
.reg_nav_wen{
    font-size: 36px;
    color: #FFFFFF;
    margin: 16px auto 29px auto;
    text-align: center;
}
.reg_nav_ner{
    width: 320px;
    margin: auto;
    padding: 30px 32px 36px 32px ;
    background: white;
    border-radius: 10px;
    position: relative;
}
.reg_nav_ner ul{
    width: 85%;
    margin: 0 auto;}
.reg_nav_ner ul li{
    width: 50%;
    float: left;
    text-align: center;
    cursor: hand;
    margin-bottom: 20px;
}
.reg_nav_ner ul li a{
    width: 100%;
    display: inline-block;
    font-size: 16px ;
    line-height: 42px;
    cursor: pointer;
}
.btn_verification{cursor: pointer;border-top-style: none;
    border-right-style: none;
    border-bottom-style: none;
    border-left-style: none;
    outline:none;}
.magtop20{
    margin-top: 20px;
}
.magtop30{
    margin-top: 30px;
}
.magtop10{
    margin-top: 10px;
}
.magtop8{
    margin-top: 8px;
}
.reg_input{
    width: 100%;
}
.reg_input1{
    font-size: 16px ;
    color: #9b9b9b;
    padding: 5px 0 5px 18px ;
    border-radius: 10px;
    border: 1px solid #d6d6d6;
}
.reg_inputtext{
    width: 298px;
    font-size: 16px ;
    padding: 11px 0 11px 18px ;
    border-radius: 10px;
    border: 1px solid #d6d6d6;
}
.reg_inputtext1{
    display: inline-block;
    /*float: left;*/
    width: 69%;
    font-size: 16px ;
    color: #9b9b9b;
}
.reg_input2{
    font-size: 16px ;
    color: #9b9b9b;
    padding: 5px 0 5px 18px ;
    border-radius: 10px;
    border: 1px solid #d6d6d6;
    position: relative;
}
.reg_input2_tuxin{
    display: inline-block;
    /*background: url("/identify")no-repeat center center;*/
    background-size: 100%;
    width: 120px;
    height: 30px;
    position: absolute;
    top: 6px;
    right: 5px;
}
.reg_input2_tuxin img{
    width: 120px;
    height: 30px;
}
.reg_inputtext2{
    display: inline-block;
    width: 49%;
    font-size: 16px ;
    color: #9b9b9b;
   line-height: 30px;
}
.btn_verification{
    display: inline-block;
    font-size: 12px ;
    /*float: left;*/
    width: 81px;
    line-height: 30px;
    border-radius: 10px;
    text-align: center;
    color: #fff;
    background: #d6d6d6;
}

.reg_nav_gcb{
    font-size: 12px ;
    color: #9b9b9b;
    margin: 13px 0;
    text-align: left;
}
.reg_denlu{
    display: block;
    width: 100%;
    line-height: 46px ;
    color: #FFFFFF;
    background: #1fbcec;
    text-align: center;
    border-radius: 10px;
    cursor:pointer;
}
.reg_wen1{
    font-size: 14px ;
    color: #9b9b9b;
    text-align: center;
}
.reg_wen2{
    display: block;
    color: #1fbcec;
    font-size: 16px ;
    text-align: center;
}
.reg_wen3{
    display: block;
    color: #fff;
    margin-top: 100px;
    font-size: 16px ;
    text-align: center;
    background: url(../images/img2/jiantou.png)no-repeat center center;
    padding-left: 78px;

}
.reg_nav_gcb a{
    display: inline-block;
    color:#1fbcec ;
}


/*按钮下 */
.buttonpress{
    color: #d1d1d1;
    background: #4a4a4a;
}
.buttonreg{
    color: #4a4a4a;
    background: rgba(209, 209, 209, 0.5);
}


.warning_box{
    width: 100%;
    text-align: center;
    line-height: 50px;
    color: #503e30;
    background:rgba(250,233,163,0.7 ) ;
    position: fixed;
    z-index: 99;
    top: 0;
}
.warning_box1{
    width: 100%;
    text-align: center;
    line-height: 50px;
    color: #503e30;
    background:rgba(250,233,163,0.7 ) ;
    position: fixed;
    z-index: 99;
    top: 0;
}
.login_forpassword{
    position: absolute;
    right: 33px;
    bottom: 17px;
    font-size: 12px;
    color: #1fbcec;
}

/*登录提示*/
.signmask{
    position: fixed;
    top: 0;
    left:0;
    bottom: 0;
    right: 0;
    background: rgba(0,0,0,0.5);
}
.signmask_div{
    width: 344px;
    height: 170px;
    margin:  300px auto 0 auto;
    background: #fff;
    border-radius: 10px;
}
.signmask_dg{
    display: inline-block;
    width: 50px;
    height: 50px;
    float: left;
    background: url(../images/imgv4/exclamatory.png)no-repeat center center;
    background-size: 100%;
    margin: 20px 10px 0 20px;
}
.signmask_vwords{
    width: 70%;
    display: inline-block;
    font-size: 16px;
    line-height: 20px;
    margin-top: 15px;
    float: left;
    text-indent:2em;
}
.signmask_btn{
    float: left;
    width: 100%;
    height: 40px;
    margin-top: 17px ;
}
.signmask_a{
    display: block;
    font-size: 16px;
    width: 60%;
    line-height: 40px;
    margin: 0 auto;
    color: #fff;
    background: #1fbcec;
    text-align: center;
    border-radius: 5px;
}