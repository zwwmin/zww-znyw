var nametxt = $('.name');
var phonetxt = $('.phone');
var pcount = xinm.length-1;//参加人数
var runing = true;
var td = 1;//内定中奖,从最小奖开始，共10个名额
var num = 0;
var t;
//开始停止
function start() {
    $("#tlite").html("便力电抽奖活动");
	if (runing) {
		runing = false;
		$('#btntxt').removeClass('start').addClass('stop');
		$('#btntxt').html('停止');
		startNum()
	} else {
		runing = true;
		$('#btntxt').removeClass('stop').addClass('start');
		$('#btntxt').html('开始');
		stop();
		// zd();//内定中奖
	}
}
//循环参加名单
function startNum() {
	num = Math.floor(Math.random() * pcount);
	nametxt.html(xinm[num]);
	phonetxt.html(phone[num].substr(0,3)+"****"+phone[num].substr(7));
	t = setTimeout(startNum, 0);
}
//停止跳动
function stop() {
	pcount = xinm.length-1;
    $("#tlite").html("恭喜获奖电工");
    $("#btntxt").hide();
	clearInterval(t);

	t = 0;
}
