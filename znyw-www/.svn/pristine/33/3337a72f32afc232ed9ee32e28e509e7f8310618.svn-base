html,body{
  background: #041B36;
}
body{
  font-family:PingFangSC-Regular;
}
ul,ol {
  list-style: outside none none;
}
input{-webkit-appearance:none; /*去除input默认样式*/ padding: 0;}
input[type="submit"],
input[type="reset"],
input[type="button"],
input{-webkit-appearance:none;}
input:focus{
  -webkit-tap-highlight-color:rgba(0,0,0,0);
  -webkit-user-modify:read-write-plaintext-only;
}
/*去除IE10+浏览器文本框后面的小叉叉*/
input::-ms-clear {
  display: none;
}
input {outline:none; border: 0;color: #4A4A4A}
input::-webkit-input-placeholder {
  /* WebKit browsers */
  color: #0E5970;
}
input::-moz-placeholder {
  /* Mozilla Firefox 4 to 18 */
  color: #0E5970;
}
input::-moz-placeholder {
  /* Mozilla Firefox 19+ */
  color: #0E5970;
}
input:-ms-input-placeholder {
  /* Internet Explorer 10+ */
  color: #0E5970;
}
@font-face {
  font-family: DS-DIGI;
  src: url('/res/css/DS-DIGI.TTF');
}

.anchorBL{display:none;}
a{text-decoration: none}
a:hover{text-decoration: none}
a:link,a:active ,a:visited,a:hover {text-decoration:none;}
li{list-style:none;}
.map_center{
  width: 100%;height: 100%;background: #041B36;min-width: 1440px;
  .header{
    width: 100%;height: 61px;float: left;background: #041B36;
    .header_logo{
      width: 30%;height: 100%;float: left;
      img{
        width: 120px;height: 39px;margin:22px 0 0 43px;
      }
    }
    .header_center{
      width: 40%;height: 100%;float: left;
      img{
        display: block;margin: 0 auto;width: 553px;height: 44px;
      }
    }
    .header_user{
      width: 30%;height: 100%;float: left;color: rgba(255,255,255,1);
      ul{
        float: right;margin-right: 27px;
      }
      li{
        padding: 10px;float: right;
        .header_user_center{
          margin-bottom: 5px;height:30px ;
        }
        .header_user_weatherText{
          font-size: 14px;text-align: center;font-family:PingFangSC;line-height: 45px;
        }
        .header_user_weatherText2{
          font-size:12px;font-family:PingFangSC;line-height: 32px;text-align: center;
        }
        .header_user_weatherImg{
          width: 47px;margin: 0 auto 5px auto;
          img{
            width: 100%;height: 100%;
          }
        }
        .header_user_tit{
          font-size: 12px;color:rgba(255,255,255,1);text-align: center;text-align: center;
          width: 100%;
          display: inline-block;
        }
        .header_user_usinfoImg{
          width: 30px;height: 30px;
          img{
            width: 100%;height: 100%;border-radius: 50%;
          }
        }
      }
    }
  }
  .center{
    width: 100%;min-height: 684px;float: left;background: #041B36;
    .center_header{
      width: 100%;height: 99px;
      span{
        display: block;font-size: 16px;color:rgba(209,209,209,1);margin-top: 15px;text-align: center;
      }
      .center_header_txt{
        margin: 0 auto;text-align: center;
        span{
          display: inline-block;text-align: center;
        }
        .special{
          font-family:DS-DIGI;font-size:36px;color:#51FFFF ;
        }
        .ordinary{
          font-size: 14px;color: #9B9B9B;
        }
      }

    }
    .center_left{
      width:302px;height: 585px;margin-left:34px;float: left;
      .center_left_Warning{
        width: 280px;height: 352px;background: url("/res/images/imgv7/znyw_dpm_left_grid1@2x.png")no-repeat;background-size:100% 100%;padding: 11px 11px;
        .center_left_Warning_header{
          width: 100%;height: 25px;
          img{
            width:25px;height: 25px;
          }
          span{
            display:inline-block;font-size: 16px;color:#F15959 ;margin-left: 7px;
          }
        }
        .center_left_Warning_center{
          width: 100%;height: 48px;margin-top: 14px;
          span{
            font-size: 20px;color: #51ffff;line-height: 48px;
          }
          .center_left_Warning_txt{
            display:inline-block;font-size: 48px;font-family:DS-DIGI;color:rgba(81,255,255,1);line-height: 48px;margin-left: 31px;
            span{
              display:inline-block;font-size: 18px;color:#298B8E ;line-height:20px;font-family:PingFangSC-Regular;
            }
          }
        }
        .center_left_chart{
          width: 100%;height: 100%;margin-top: 60px;
          .center_left_chart_nav_right{
            margin-right: 18px;
          }
          .center_left_chart_nav{
            width:81px;height:200px;float: left;
            .chart_text{
              width: 100%;margin-top: 23px;
              span{
                display:block;color: white;text-align: center;
              }
              .chart_text_span1{
                font-size: 30px;font-family:DS-DIGI;line-height: 30px;
              }
              .chart_text_span2{
                font-size: 12px;line-height: 12px;margin-top: 9px;cursor:pointer;
              }
            }
          }
        }
      }
      .center_left_tole_right{margin-right: 15px;}
      .center_left_tole{
        width: 143px;height: 183px;margin-top: 32px;background:  url("/res/images/imgv7/znyw_dpm_left grid2@2x.png")no-repeat;background-size:100% 100%;float: left;
        .center_left_tole_text{
          font-size: 20px;padding: 10px 0 0 11px;text-align: left;color: #51FFFF;
        }
        .center_left_tole_num{
          font-size: 48px;font-family:DS-DIGI;padding-left: 19px;margin-top:9px;color: #51FFFF;
          span{
            font-size: 20px;font-family:PingFangSC-Regular;
          }
        }
        img{
          display: block;width: 80px;height: 80px;margin: 8px auto 0 auto;
        }
      }
    }
    .center_map{
      width: calc(100% - 570px);height: 585px;margin: 0 auto;float: left;overflow: hidden;
    }
    .center_right{
      width:212px;height:  585px;margin-right: 15px;float: right;
      .center_right_header{
        width: 212px;height: 93px;background: url("/res/images/imgv7/znyw_dpm_right grid1@2x.png")no-repeat;background-size: 100%;
        .center_right_header_wen{
          width: calc(100% - 80px) ;height:100%;float: left;
          span{
            display:inline-block;font-size: 20px;color: #51FFFF;margin: 11px 0 0 14px;
          }
          .header_wen{
            font-size:48px;font-family:DS-DIGI;color: #51FFFF;margin: 5px 0 10px 14px;
            span{
              font-size: 20px;color:#298B8E ;font-family:PingFangSC-Regular;
            }
          }
        }
        .center_right_header_img{
          width: 70px;height: 76px;margin: 15px 8px 0 0 ;background: url("/res/images/imgv7/znyw_dpm_warming mes@2x.png") no-repeat;background-size:100%;float: left;
        }
      }
      .SearchFor{
        width: 212px;height: 429px;margin-top: 10px;text-align: left;background: url("/res/images/imgv7/znyw_dpm_right grid2@2x.png")no-repeat ;background-size: 100%;
        input{
          height: 40px;padding-left:44px;background: url("/res/images/imgv7/znyw_dpm_search@2x.png")no-repeat 20px 11px;background-size: 18px 18px;font-size: 16px;line-height: 40px;color: #1A8DB0;
        }
        ul{
          width: 100%;height: 390px;padding-top:16px;
          li{
            height:18px;font-size: 14px;margin-bottom:12px;padding-left: 40px;
            a{
              color: #B2B2B2;line-height: 14px;text-overflow:ellipsis;white-space:nowrap;overflow:hidden; cursor:pointer;
            }
          }
        }
      }
    }
  }
  .footer{
    width: 100%;height: 135px;float:left;background: url("/res/images/imgv7/znyw_dpm_down2 background@2x.png")no-repeat;background-size: 100%;
    .footer_center{
      width: calc(100% - 820px);height: 105px;margin: -10px auto 0 auto;background:url("/res/images/imgv7/znyw_dpm_down1background@2x.png")no-repeat ;background-size: 100%  100%;
    }
  }

}
.mask_mappage{
  z-index: 9998;
  background-color:rgba(4,16,41,1) ;
  opacity:0.6953 ;
}
.mask_center_mappage{
  z-index:9999 ;width: 100%;height: 100%;position: fixed;top:0;left:0;right: 0;bottom: 0;background-color:rgba(4,16,41,1) ;
  .center_mappage{
    width:964px;margin: 8% auto 0 auto;background: url("/res/images/imgv7/ypt_tk background@2x.png")no-repeat;background-size: 100% 100%;position: relative;padding: 30px;
    .close{
      width: 50px;height: 50px;background: url("/res/images/imgv7/ypt_close icon@2x.png")no-repeat;background-size: 100% 100%;position: absolute;top: -25px;right: -25px;
    }
    .center_mappage_map{
      width: 100%;height: 320px;
    }
    .center_mappage_nav{
      width: 100%;overflow: auto;
      ul{ overflow-y: hidden; }
      li{
        width: 100%;height:104px;background-color: white; border-bottom:2px solid rgba(214,214,214,1) ;
        .mappage_nav_left{
          width: calc(70% - 20px);height: 100%;float: left;padding-left: 20px;
          .mappage_nav_top{
            font-size: 18px;color:rgba(0,0,0,1);line-height:16px;margin-top: 20px;
            .mappage_com{margin-right: 15px;}
            .mappage_icon{
              padding-left:15px; background: url("/res/images/imgv7/ypt_contact@2x.png")no-repeat left;background-size: 12px 15px;
            }
            .mappage_icon2{
              padding-left:15px; background: url("/res/images/imgv7/ypt_mobile phone@2x.png")no-repeat left;background-size: 12px 15px;
            }
          }
          .mappage_nav_center{
            margin-top: 8px;
            span{
              margin-right: 30px;color: #9B9B9B;
            }
          }
        }
        .mappage_nav_right{
          width: calc(30% - 20px);height: 100%;float: left;padding-right: 20px; cursor:pointer;
            span{
              padding-right:24px; background: url("/res/images/imgv7/ypt_inter@2x.png")no-repeat right;background-size:18px 20px;line-height:104px;float: right;font-size: 16px;color:rgba(74,74,74,1);
            }

        }
      }
    }
    .center_mappage_nav::-webkit-scrollbar {display:none}
    .center_mappage_marn{
      width:100%;height: 628px;background:rgba(255,255,255,1);
    }
  }

}