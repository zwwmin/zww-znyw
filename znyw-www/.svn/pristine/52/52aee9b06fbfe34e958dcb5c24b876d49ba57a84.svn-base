/*案例列表*/
.case_div{
    width: 100%;position: relative;height: 175px;margin-bottom: 10px;display: block;
}
.case_div_bg{
    width: 100%;height:37px;position: absolute;bottom: 0;left: 0;z-index: 2;padding: 0 14px 0 22px;
    /*background: url("../images/imgv4/shadow_icon@3x.png")no-repeat center center;*/
    background: rgba(0,0,0,0.3);
}
.case_div_bg_span1{
    display: inline-block;font-size: 14px;color: white;position: absolute;bottom: 12px;left:22px;
}
.case_div_bg_span2{
    display: inline-block;font-size: 12px;color: white;position: absolute;bottom: 13px;right: 14px;
}
/*案例与评价*/
.example_app_body{
    width: 100%;
    height: 100%;
    min-width:0;
}
.exampleapp_center{
    margin: 0 auto;
    height: 100%;
    padding:30px 12px 0 12px ;
}
.exampleapp_top{
    width: 100%;
    float: left;
}
h1{
    width: 69%;
    font-size: 20px;
    color: #000;
    float: left;
    margin: 0;
    padding: 0;
}
.exampleapp_top_appspan1{
    display: block;
    float: right;
    height: 24px;
    width: 50px;
    background: url("../images/imgv4/shiyan_icon.png") no-repeat center center;
    background: #00a0e9;
    background-size:100% ;
}
.exampleapp_top img{
    width: 90px;
    height: 26px;
    float: right;
}
.exampleapp_nav{
    width: 100%;
    margin-top: 27px;
    float: left;
}
.exampleapp_nav span{
    font-size: 14px;
    display: block;
    color: #4A4A4A;
}
.exampleapp_describe{
    width: 100%;
    margin-top:30px ;
}
h2{
    font-size: 14px;
    line-height: 20px;
    margin: 0;
    padding: 0;
}
.exampleapp_describe_nav{
    font-size:14px ;
    line-height: 20px;
    color: #4a4a4a;
}
.exampleapp_describe_nav img{
    width: 100%;
    height: 100%;
}
.exampleapp_describe_img img{
    margin-top:10px;
    width: 100%;
    height: 100%;
}
.exampleapp_pj{
    width: 100%;
    float: left;
    background: white;
}
.Evaluation{
    position: relative;
}
/*外派订单页面*/
.weixin_Record{
    width: 92%;padding-left: 4px;overflow: hidden;margin: 0 auto;
}
.Record_nev{
    display: block;line-height: 26px;font-size: 14px;color: #9b9b9b;
}
.Record_nev_xia{
    padding-left: 5px;
}
.Record_nev_img img{
    width: 45%;
}
.Remarks_input{
    display:inline-block;width:75% ;margin:10px auto;font-size: 16px;border: 1px solid #d6d6d6;border-radius: 10px;line-height: 30px;padding-left: 15px;float: left;
}
.Remarks_btn{
    display: inline-block;width: 100%;float: left;font-size: 16px;line-height: 34px;background: #1fbcec;color: white;text-align: center;margin: 10px auto 10px auto;border-radius: 5px;
}