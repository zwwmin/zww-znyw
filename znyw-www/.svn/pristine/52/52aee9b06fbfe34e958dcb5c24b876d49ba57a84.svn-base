@charset "utf-8";

body,h1,h2,h3,p,ul,ol,form,fieldset,figure {
    margin: 0;
    padding: 0;
}
div,figure,img {
    box-sizing: border-box;
}
body {
    background-color: #f8f8f9;
    font-family: "Helvetica Neue", Helvetica, Arial, "Microsoft Yahei UI", "Microsoft YaHei", SimHei, "\5B8B\4F53", simsun, sans-serif;

}
input:focus{
    color: #4A4A4A;
}
input,
button,
select,
textarea {
    outline: none;
    border: 0;
}
ul,ol {
    list-style: outside none none;
}
input{-webkit-appearance:none; /*去除input默认样式*/}
input[type="submit"],
input[type="reset"],
input[type="button"],
input{-webkit-appearance:none;}
input:focus{
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    -webkit-user-modify:read-write-plaintext-only;
}
/*去除IE10+浏览器文本框后面的小叉叉*/
input::-ms-clear {
    display: none;
}
input {outline:none; border: 0;color: #4A4A4A}
input::-webkit-input-placeholder {
    /* WebKit browsers */
    color: #9b9b9b;
}
input::-moz-placeholder {
    /* Mozilla Firefox 4 to 18 */
    color: #9b9b9b;
}
input::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color: #9b9b9b;
}
input:-ms-input-placeholder {
    /* Internet Explorer 10+ */
    color: #9b9b9b;
}
a{text-decoration: none}
a:hover{text-decoration: none}
textarea {
    resize: none
}

select::-ms-expand {
    display: none;
}

select {
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
}
.dis{display: none;}
/*蓝帽子入驻*/
.lmz_center{
    width: 100%;
    height: 100%;
}
.lmz_center_top{
    position: relative;
    height: 627px;
    background: url("../images/imgv4/lmz_banner.png") no-repeat center center;
}
.lmz_center_top_nav{
    height: 443px;
    margin: 0 auto;
    background: url("../images/imgv4/triangle.png") no-repeat center center;

}

.lmz_center_middle{
    width: 100%;
    height: 690px;
    background: #1b172f;
}
.center_middle_center{
    padding-top:67px ;
    max-width: 1200px;
    margin: 0 auto;
}
.center_middle_nav{
    padding-left: 168px;
    height: 249px;
}
.process_liuc_img{
    float: left;
    margin-bottom: 27px;
}
.middle_process_liuc_img{
    width: 50px;
    height: 50px;
    margin-left: 10px;
}
.middle_process_liuc_img1{
    background: url("../images/imgv4/application.png") no-repeat center center;
}
.middle_process_liuc_img2{
    background: url("../images/imgv4/download.png") no-repeat center center;
}
.middle_process_liuc_img3{
    background: url("../images/imgv4/send.png") no-repeat center center;
}
.middle_process_liuc_img4{
    background: url("../images/imgv4/audit.png") no-repeat center center;
}
.middle_process_liuc_img5{
    background: url("../images/imgv4/Education.png") no-repeat center center;
}
.middle_process_liuc_img6{
    background: url("../images/imgv4/join.png") no-repeat center center;
}
.arrowhead{
    float: left;
    height: 9px;
    width: 50px;
    margin: 62px 10px 0 10px ;
    background: url("../images/imgv4/right.png") no-repeat center center;;
}
.arrowhead2{
    float: left;
    height: 9px;
    width: 50px;
    margin: 62px 5px 0 5px ;
    background: url("../images/imgv4/right.png") no-repeat center center;;
}
.process_liuc_span{
    margin-top: 5px;
    display: block;
    font-size: 16px;
    color: #fff;
}
.float_right{float: left;}
.deg{
    transform: rotate(0deg);
    -o-transform: rotate(0deg);
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
}
.lmz_email{
    float: left;
}
.lmz_email_img{
    float: left;
    width: 30px;
    height: 26px;
    margin-right: 15px;
    background: url("../images/imgv4/finger.png") no-repeat center;;
}
.lmz_email_wen{
    display: inline-block;
    font-size: 16px;
    color: #fff;
    float: left;
}
.lmz_email_wen span{
    display: inline-block;
    margin-left: 5px;
    font-size: 16px;
    color: #1fbcec;
}
/*表單*/
.lmz_center_from{
    width: 100%;
    height: 650px;
    background: #f8f8f9;
}
.center_from_center{
    max-width: 1200px;
    height: 100%;
    margin: 0 auto;
}
.center_from_div{
    width: 100%;
    height: 70%;
    padding:80px 0 0 168px ;
}
.center_from_wen1{
    font-size: 24px;
    color: #000;
    line-height: 30px;
}
.center_from_wen2{
    font-size: 18px;
    color: #000;
    line-height: 30px;
}
.magright15{margin-right: 15px}
.magbottom{margin-bottom: 10px}
.from_input{
    font-size: 16px;
    line-height: 25px;
    float: left;
    padding:6px 0 4px 18px;
    background: #fff;
}
.input_width{width: 280px}
.from_input_yzm{
    line-height: 25px;
    float: left;
    width: 56%;
    font-size: 16px;
}
.from_input_post{
    display: inline-block;
    float: left;
    text-align: center;
    font-size: 14px;
    line-height: 27px;
    width: 35%;
    color: #1fbcec;
    background: #ebf3f6;
}
.lmz_submit{
    display: block;
    width: 120px;
    height: 40px;
    float: left;
    font-size: 16px;
    background: #1FBCEC;
    padding-left: 15px;
    position: relative;
}
.lmz_submit span{
    display: inline-block;
    line-height: 40px;
    font-size: 16px;
    color: #fff;
}
.lmz_submit_arrowhead{
    position: absolute;
    right: 20px;
    width: 26px;
    height: 18px;
    display: inline-block;
    background-size: 100%;
    margin-top: 12px;
    background:url("../images/imgv4/right_white_icon.png") no-repeat center center ;
}
.lmz_center_from_back{
    max-width: 1200px;
    background: url("../images/imgv4/city blue.png") no-repeat ;
    background-position-y:bottom;
}
/*按钮变手*/
.lmz_submit
,.lmz_center_top_buttom{
    cursor:pointer ;
}
.app_email{
    width: 90%;
    height: 215px;
    position: fixed;
    top:25%;
}
.email{
    width: 100%;
    height: 230px;
    border-radius:5px;
    margin: 0 auto;
    background: #fff;
    padding: 10px 20px 10px 20px;
}
.email span{
    font-size: 12px;color: red;display: inline-block;margin-top: 5px;
}
.email input{
    width: 90%;
    margin: 15px auto 0 auto;
    font-size: 18px;
    padding: 5px 0 5px 15px;
    line-height: 25px;
    color: #000000;
    background: #f8f8f9;
}
.lmz_btn{
    display: block;
    width: 96%;
    font-size: 18px;
    line-height: 40px;
    margin: 15px auto 0 auto;
    background: #1FBCEC;
    border-radius: 5px;
    color: #fff;
    text-align: center;
}
.lmz{
    animation: lmz 1s;
    -moz-animation: lmz 1s;
    animation-fill-mode:forwards;
    -webkit-animation-fill-mode:forwards;
}
@-moz-keyframes lmz{
    0%{
        transform: translateY(-200px);
        opacity: 0;
    }
    100%{
        transform: translateY(0px);
        opacity: 1;
    }
}
@keyframes lmz{
    0%{
        transform: translateY(-200px);
        opacity: 0;
    }
    100%{
        transform: translateY(0px);
        opacity: 1;
    }
}

.download {
    width:100%;
    height: 300px;
    position: fixed;
    top: 340px;

}
.download_div{
    width: 280px;
    height: 180px;
    margin: 0 auto;
    background: #f8f8f9;
    padding: 20px 20px;
    border-radius: 5px;
}
.download_div_ti{
    width: 280px;
    margin: 0 auto;
    background: #f8f8f9;
    padding: 20px 20px;
    border-radius: 5px;
}
.download_div_ti p{
   text-align: center;
}
.download_div p{
    text-indent:2em;
}
.download_div a{
    display: block;
    width: 80%;
    margin: 20px auto 0 auto;
    font-size: 18px;
    line-height: 40px;
    border-radius: 5px;
    color: #fff;
    background: #1FBCEC;
    text-align: center;
}
.download_email{
    display: inline-block;
    color: #1FBCEC;
}
/*头部*/
.nav_header_v4{width: 100%;}
.headerV4_content{max-width: 1400px;z-index: 99; height: 50px;position: relative;margin: 0 auto;padding: 0 40px;}
.headerV4_logo{float: left;width: 108px;height: 37px;background: url("../images/img/logo_s.png") no-repeat center ;background-size: 100%;margin-top: 7px;}
.headerV4_content_nav{ margin-left: 55px}
.headerV4_city{line-height: 50px;display: inline-block;font-size: 16px !important;float: left;color: #000;margin: 0 40px}
.headerV4_content_nav li{float: left;margin-right: 30px;}
.headerV4_content_nav li a{display: inline-block;font-size: 16px;color: #000;line-height: 50px;}
.headerV4_content_nav li .on{color: #1fbcec;line-height: 47px;border-top: 3px solid #1fbcec;}
.headerV4_content_nav li a:hover{color: #1fbcec;box-sizing:border-box;line-height: 47px;border-top: 3px solid #1fbcec;}
.headerV4_content_right{float: right;  }
.headerV4_right_nav{display:inline-block;float: right;color: #000;font-size: 16px;margin-left: 15px;line-height: 50px;}
.headerV4_right_nav_reg{display: inline-block;float: right;color:#1fbcec;width: 72px;height: 37px;background: url("../images/img2/Rectangletop@1x.png") no-repeat center center; line-height: 37px;text-align: center;margin-top: 7px;font-size: 16px; margin-left: 12px; }
.headerV4_right_nav:hover{color: #1fbcec;}
.headerV4_right_nav_reg:hover{color: #5d4732;}

