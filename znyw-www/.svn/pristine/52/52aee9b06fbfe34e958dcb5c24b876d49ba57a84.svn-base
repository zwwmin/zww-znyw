/*工程公司入驻动画*/
.download_lor {
    animation: comdownloadlor 1s;
    -moz-animation: comdownloadlor 1s;
    animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}
@-moz-keyframes comdownloadlor {
    0% {
        transform: translatex(200px);
        opacity: 0;
    }
    100% {
        transform: translatex(0px);
        opacity: 1;
    }
}

@keyframes comdownloadlor{
    0% {
        transform: translatex(200px);
        opacity: 0;
    }
    100% {
        transform: translatex(0px);
        opacity: 1;
    }
}
/*出去*/
.download_xia {
    animation: comdownload 2s;
    -moz-animation: comdownload 2s;
    animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}

@-moz-keyframes comdownload {
    0% {
        transform: translatex(0px);
        opacity: 1;
        display: block;
    }
    100% {
        transform: translatex(-200px);
        opacity: 0;
        display: none;
    }
}

@keyframes comdownload {
    0% {
        transform: translatex(0px);
        opacity: 1;
        display: block;
    }
    100% {
        transform: translatex(-200px);
        opacity: 0;
        display: none;
    }
}

/*反回去*/
.download_sosnoe {
    animation: comdownloadsos 2s;
    -moz-animation: comdownloadsos 2s;
    animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}

@-moz-keyframes comdownloadsos {
    0% {
        transform: translatex(0px);
        opacity: 1;
    }
    100% {
        transform: translatex(200px);
        opacity: 0;
    }
}
@keyframes comdownloadsos {
    0% {
        transform: translatex(0px);
        opacity: 1;
    }
    100% {
        transform: translatex(200px);
        opacity: 0;
    }
}
.download_sostwo {
    animation: comdownloadsost 1s;
    -moz-animation: comdownloadsost 1s;
    animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
}

@-moz-keyframes comdownloadsost {
