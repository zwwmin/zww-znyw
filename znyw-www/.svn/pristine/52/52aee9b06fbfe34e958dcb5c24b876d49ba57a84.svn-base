        transform: translatex(-200px);
        opacity: 0;
    }
    100% {
        transform: translatex(0px);
        opacity: 1;
    }
}

@keyframes comdownloadsost {
    0% {
        transform: translatex(-200px);
        opacity: 0;
    }
    100% {
        transform: translatex(0px);
        opacity: 1;
    }
}
