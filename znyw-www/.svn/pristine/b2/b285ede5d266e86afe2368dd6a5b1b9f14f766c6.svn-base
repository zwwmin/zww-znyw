#comapp,
#service{cursor:pointer;}
.download_xia{
    animation: comdownload 1s;
    -moz-animation: comdownload 1s;
    animation-fill-mode:forwards;
    -webkit-animation-fill-mode:forwards;
}
@-moz-keyframes comdownload {
    0%{
        transform: translateY(50px);
        opacity: 0;
    }
    100%{
        transform: translateY(0px);
        opacity: 1;
    }
}
@keyframes comdownload {
    0%{
        transform: translateY(50px);
        opacity: 0;
    }
    100%{
        transform: translateY(0px);
        opacity: 1;
    }
}
span{display: block;}