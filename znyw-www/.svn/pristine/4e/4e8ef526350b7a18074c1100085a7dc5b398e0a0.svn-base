/***缩写为hmiu**/
.html5-multi-img-uploader{

}
.hmlu{
    width:100%;
    height: auto;

}
.hmlu:after{
    clear: both;
    display: block;
    width: 0px;
    height: 0px;
    content:"";
}
.hmlu .hmlu_img_item{
    float: left;
    width:70px;
    height:90px;
    position: relative;
    margin-right: 10px;

}
.hmlu .hmlu_pre_img{
    width: 70px;
    height: 70px;
    border: 0px;

}
.hmlu .hmlu_mask{
    width:70px;
    height: 70px;
    background: rgba(0,0,0,0.4);
    position: absolute;
    left:0;
    top:0;
}

.hmlu .hmlu-load-bar {
    width: 100%;
    height: 10px;
    top:30px;
    left:0;

    border-radius: 30px;
    background: #dcdbd7;
    position: absolute;
    box-shadow: 0 1px 0 rgba(255, 255, 255, 0.8),  inset 0 2px 3px rgba(0, 0, 0, 0.2);
}

.hmlu .hmlu-load-bar-inner {
    height: 99%;
    width: 0%;
    border-radius: inherit;
    position: relative;
    background: #c2d7ac;
    background: linear-gradient(#e0f6c8, #98ad84);
    background: -moz-linear-gradient(#e0f6c8, #98ad84);
    background: -webkit-linear-gradient(#e0f6c8, #98ad84);
    background: -o-linear-gradient(#e0f6c8, #98ad84);
    box-shadow: inset 0 1px 0 rgba(255, 255, 255, 1),  0 1px 5px rgba(0, 0, 0, 0.3),  0 4px 5px rgba(0, 0, 0, 0.3);
    animation: loader 10s linear infinite;
    -moz-animation: loader 10s linear infinite;
    -webkit-animation: loader 10s linear infinite;
    -o-animation: loader 10s linear infinite;
}



.hmlu .hmlu-success-tips{
    width: 100%;
    height: 18px;
    overflow: hidden;
    position: absolute;
    left: 0;
    top: 30px;
    background: url("images/check_success.png") 1px center no-repeat;
    background-size: 14px 14px;
    padding-left: 16px;
    line-height: 18px;
    color: white;
    font-size: 12px;
    font-weight: bold;
}
.hmlu .hmlu-error-tips{
    width: 100%;
    height: 18px;
    overflow: hidden;
    position: absolute;
    left: 0;
    top: 30px;
    background: url("images/check_error.png") 1px center no-repeat;
    background-size: 14px 14px;
    padding-left: 16px;
    line-height: 18px;
    color: orangered;
    font-size: 12px;
    font-weight: bold;
}
.hmlu .hmlu-normal-tips{
    width: 100%;
    height: 18px;
    overflow: hidden;
    position: absolute;
    left: 0;
    top: 30px;
    background: url("images/check_info.png") 1px center no-repeat;
    background-size: 14px 14px;
    padding-left: 16px;
    line-height: 18px;
    color: white;
    font-size: 12px;
    font-weight: bold;
}
.hmlu .hmlu-bottom-bar{
    position: absolute;
    width: 100%;
    left: 0;
    bottom: 0;
    text-align: center;
    height: 20px;
}
.hmlu .hmlu-bottom-bar .hmlu-bottom-tips{
    font-size: 12px;
    text-align: center;

    color: #666666;
    letter-spacing: 1px;
    line-height: 20px;
    text-overflow:ellipsis;
    -o-text-overflow:ellipsis;
    -webkit-text-overflow:ellipsis;
    -moz-text-overflow:ellipsis;
    white-space:nowrap;
    overflow: hidden;

}
.hmlu .hmlu-bottom-bar .hmlu-del-button{
    font-size: 12px;
    text-align: center;
    background: url("images/trash.png") left center no-repeat;
    padding-left: 16px;
    text-decoration: none;

    color: #666666;
    letter-spacing: 2px;
    line-height: 20px;
}
.hmlu .hmlu-upload-button{
    background: url("images/addpic.png") left top no-repeat;
    background-size: 70px 70px;
    float: left;
    width: 70px;
    height: 80px;
    overflow: hidden;
    position: relative;
}
.hmlu .hmlu-upload-button .hmlu-input-file{
    position: absolute;
    font-size: 100px;
    right: 0;
    top: 0;
    opacity: 0;
    filter: alpha(opacity=0);

    cursor: pointer
}