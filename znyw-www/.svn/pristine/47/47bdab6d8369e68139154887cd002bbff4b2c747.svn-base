    <!--点击打开right的侧边栏-->
    <!--<div class="control-sidebar-bg"></div>-->


</div>
<% include layout/inc_js.ejs%>
<script>

    var pageOptions={};
    seajs.use("p.Intelligent_energy",function(page){
        page.init(pageOptions);
    });
</script>
</body>
</html>