/*员工提示*/
.staffprompt{width: 242px;height: 170px;box-shadow:4px -1px 9px 1px rgba(0,0,0,0.1) ;position: fixed;top:35%;left: 45%;padding-top: 21px;background: #ffffff;border-radius: 5px;overflow: hidden;}
.staffprompt img{width: 50px;height: 50px;margin: 0 auto;border-radius: 50%;overflow: hidden;display: block;}
.staffprompt span{font-size:14px;color: #4a4a4a;margin-top: 17px;text-align: center; }
.backclose{background: url("../images/imgv4/backclose1.png") no-repeat center;width: 20px;height: 20px;background-size: 100%;position: absolute;top: 5px;right: 5px;cursor: pointer;}
.backclose:hover{background: url("../images/imgv4/backclose2.png") no-repeat center;width: 20px;height: 20px;background-size: 100%;}
.staffprompt a{display: block;width: 100%;height: 53px;margin-top: 31px;font-size: 14px;color: #fff;background: #484756;text-align: center;line-height: 50px;border-radius:5px; }
.backs{
    animation: back 1.5s;
    -moz-animation: back 1.5s;
    animation-fill-mode:forwards;
    -webkit-animation-fill-mode:forwards;
}
@-moz-keyframes back{
    0%{
        transform: translateY(-500px);
        opacity: 0;
    }
    100%{
        transform: translateY(0px);
        opacity: 1;
    }
}
@keyframes back{
    0%{
        transform: translateY(-500px);
        opacity: 0;
    }
    100%{
        transform: translateY(0px);
        opacity: 1;
    }
}
