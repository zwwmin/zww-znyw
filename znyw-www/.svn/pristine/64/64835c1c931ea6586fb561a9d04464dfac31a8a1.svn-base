/*提示框*/
.jq-error {
    font-size: 14px;
    min-width: 280px;
    width: auto;
    max-width: 280px;
    line-height: 26px;
    position: relative;
    border: 1px solid #fdf2d5;
    background-color: #fdf2d5;
    color: #ff7901;
    margin-top: 5px;
    padding: 4px 10px;
}
.error-bottom>div {
    width: 8px;
    height: 8px;
    position: absolute;
    background-color: #fdf2d5;
    transform: rotate(45deg);
    margin-top: -7px;
}
.error-top {
    position: absolute;
    margin-top: -67px;
}

.error-top>div {
    width: 8px;
    height: 8px;
    position: absolute;
    background-color: #fdf2d5;
    transform: rotate(45deg);
    bottom: -5px;
}

.error-right {
    float: right;
    margin-left: 10px;
    margin-top: 0;
    /*position: absolute;*/
}
.error-right>div {
    width: 8px;
    height: 8px;
    position: absolute;
    background-color: #fdf2d5;
    transform: rotate(45deg);
    left: -5px;
    margin-top: 5px;
}
