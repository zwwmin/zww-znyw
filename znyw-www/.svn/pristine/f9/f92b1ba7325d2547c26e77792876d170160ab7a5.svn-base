html,
body {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
}

body {
    Font-Family: "Hiragino Sans GB", "Microsoft Yahei", SimSun, Arial, "Helvetica Neue", Helvetica;
}

input,
button,
select,
textarea {
    outline: none;
    border: 0;
}
ul{list-style: none}
a{text-decoration: none}
a:hover{text-decoration: none}
textarea {
    resize: none
}

select::-ms-expand {
    display: none;
}

select {
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
}

.matrig27 {
    margin-right: 27px;
}

.matbot25 {
    margin-bottom: 25px;
}

.matbot15 {
    margin-bottom: 15px;
}

.matbot30 {
    margin-bottom: 30px;
}

.back {
    width: 100%;
    height: 100%;
    min-height: 100%;
    background: url("../images/img2/mountain.png")no-repeat center center;
}
.total{
    width: 100%;
    height: 100%;
}
/*头部*/
.lmz_header{
    width: 100%;
    height: 50px;
    background: #FFFFFF;
}
.lmz_header_nav{
    max-width: 1200px;
    margin: 0 auto;
    height: 100%;
}
.lmz_header_logo{
    width: 108px;
    height: 35px;
    margin-top: 7px;
    float: left;
    display: inline-block;
    margin-left: 35px;
    background: url("../images/img2/logohei@1x.png") no-repeat center center;
}
.lmz_header_nav ul{
    float: left;
    margin: 0 0 0 100px;
}
.lmz_header_nav ul li{
    float: left;
    margin-top: 3px;
    margin-left: 30px;
}
.lmz_header_nav ul li a{
    display: inline-block;
    font-size: 18px;
    color: #000000;
    line-height: 47px;
}
.lmz_header_nav ul li:hover{
    border-top:3px solid #1FBCEC ;
    margin-top: 0;
    color: #1FBCEC;
}

/*中部*/
.lmz_center{
    width: 100%;
    height: 100%;
}
.lmz_center_top{
    position: relative;
    height: 627px;
    background: url("../images/imgv4/lmz_banner.png") no-repeat center center;
}
.lmz_center_top_nav{
    width: 803px;
    height: 443px;
    margin: 0 auto;
    background: url("../images/imgv4/triangle.png") no-repeat center center;
}
.lmz_center_middle{
    width: 100%;
    height: 691px;
    background: #1b172f;
}
.center_middle_center{
    padding-top:67px ;
    max-width: 1200px;
    margin: 0 auto;
}
.center_middle_nav{
    padding-left: 168px;
    height: 249px;
}
.center_middle_nav_lmzlogo{
    width:20% ;
    float: left;
    height: 161px;
    padding: 41px 50px 47px 20px;
    background: url("../images/imgv4/lmzlogo.png") no-repeat center center;
}
.center_middle_nav_wenzi{
    padding-top:41px ;
    width: 60%;
    float: left;
}
.center_middle_nav_wenzi span{
    font-size: 24px;
    color: #FFFFFF;
    text-align: left;
}
.center_middle_nav_wenzi p{
    margin: 22px 0 43px 0 ;
    font-size: 16px;
    color: #FFFFFF;
    text-align: left;
}
.center_middle_nav_wenzi a{
    color: #1FBCEC;
    font-size: 16px;
}
.center_middle_process{
    padding: 105px 0 0 168px ;
}
.center_middle_process_span{
    display: inline-block;
    float: left;
    font-size: 24px;
    margin-right: 60px;
    padding-top: 20px;
    color: #fff;
    margin-bottom: 20px;
}
.process_liuc_img{
    float: left;
    margin-bottom: 27px;
}
.middle_process_liuc_img{
    width: 50px;
    height: 50px;
    margin-left: 10px;
}
.middle_process_liuc_img1{
    background: url("../images/imgv4/application.png") no-repeat center center;
}
.middle_process_liuc_img2{
    background: url("../images/imgv4/download.png") no-repeat center center;
}
.middle_process_liuc_img3{
    background: url("../images/imgv4/send.png") no-repeat center center;
}
.middle_process_liuc_img4{
    background: url("../images/imgv4/audit.png") no-repeat center center;
}
.middle_process_liuc_img5{
    background: url("../images/imgv4/Education.png") no-repeat center center;
}
.middle_process_liuc_img6{
    background: url("../images/imgv4/join.png") no-repeat center center;
}
.process_liuc_span{
    margin-top: 5px;
    display: block;
    font-size: 16px;
    color: #fff;
}
.lmz_center_from{
    width: 100%;
    height: 500px;
    background: #f8f8f9;
}

.center_from_center{
    max-width: 1200px;
    margin: 0 auto;
}
.center_from_div{
    padding:80px 0 0 168px ;
}

.center_from_wen1{
    font-size: 24px;
    color: #000;
    line-height: 30px;
}
.center_from_wen2{
    font-size: 18px;
    color: #000;
    line-height: 30px;
}



.from{
    width: 670px;
    height: 145px;
    margin-top: 50px;
}
.magright15{margin-right: 15px}
.magbottom{margin-bottom: 10px}
.from_input{
    font-size: 16px;
    line-height: 25px;
    float: left;
    padding:6px 0 6px 18px;
    background: #fff;
}
.from_input_lmz{
    line-height: 25px;
    width: 260px;
    font-size: 16px;
    color: #D1D1D1;
}

.from_input_yzm{
    line-height: 25px;
    float: left;
    width: 64%;
    font-size: 16px;
}
.from_input_post{
    display: inline-block;
    float: left;
    text-align: center;
    font-size: 14px;
    width: 35%;
    color: #acacac;
    background: #f8f8f9;
}
.lmz_submit{
    display: block;
    width: 120px;
    height: 40px;
    float: left;
    margin-top: 15px;
    font-size: 16px;
    background: #1FBCEC;
    padding-left: 15px;
    position: relative;
}
.lmz_submit span{
    display: inline-block;
    line-height: 40px;
    font-size: 16px;
    color: #fff;
}
.lmz_submit_arrowhead{
    position: absolute;
    right: 20px;
    width: 26px;
    height: 18px;
    display: inline-block;
    background-size: 100%;
    margin-top: 12px;
    background:url("../images/imgv4/right_white_icon.png") no-repeat center center ;
}
.lmz_center_from_back{
    width: 1200px;
    height: 199px;
    background: url("../images/imgv4/city blue.png") no-repeat center center;
}




.container_wen {
    display: block;
    font-size: 36px;
    text-align: center;
    color: white;
    margin: 41px auto 40px auto;
}

.select select {
    color: #D1D1D1;
    width: 100%;
    height: 100%;
    border-radius: 5px;
    border: 0;
    background: url("../images/img/z_arrow_down.png")no-repeat 90px center rgba(128, 127, 138, 0.5);
    padding-left: 14px;
}

option {
    background: rgba(83, 82, 93, 0.62);
    color: rgba(219, 219, 219, 0.85);
    border: 0;
}

.container_btn2 {
    display: block;
    font-size: 16px;
    background: #1FBCEC;
    padding: 21px 189px 21px 189px;
    color: white;
    float: left;
    border-radius: 5px;
}


